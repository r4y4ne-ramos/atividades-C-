#include <iostream>
using namespace std;

int main() {
    int nota;
    cout << "Digite uma nota de 0 a 10: ";
    cin >> nota;

    switch (nota) {
        case 0: case 1: case 2: case 3: case 4:
            cout << "Insuficiente" << endl;
            break;
        case 5: case 6:
            cout << "Regular" << endl;
            break;
        case 7: case 8:
            cout << "Bom" << endl;
            break;
        case 9: case 10:
            cout << "Ótimo" << endl;
            break;
        default:
            cout << "Nota inválida!" << endl;
    }
    return 0;
}