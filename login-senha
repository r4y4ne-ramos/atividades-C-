#include <iostream>
#include <string>
using namespace std;

int main()
{
    string usuario, senha;
    bool acessoPermitido;
    
    cout << "Digite seu nome de usuario:" ;
    cin >>  usuario;
    
    cout << "Digite sua senha: ";
    cin >> senha;
    
    acessoPermitido = (usuario == "Rayane" && senha == "123");
    
    cout << "Acesso permitido: " <<(acessoPermitido ? "verdadeiro" : "falso") <<endl;
    

    return 0;
}